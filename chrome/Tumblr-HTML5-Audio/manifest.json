{
   "name": "Tumblr HTML5 Audio",
   "version": "1.2",
   "manifest_version": 2,
   "description": "Lets you listen to audio posts on your Tumblr Dashboard without Flash.",
   "update_url": "https://github.com/jesstech/Tumblr-HTML5-Audio/raw/master/chrome/update-manifest.xml",
   "content_scripts": [
    {
      "matches": ["*://*.tumblr.com/*"],
      "js": ["jquery-1.9.1.min.js", "audio.js"]
    }
   ]
}